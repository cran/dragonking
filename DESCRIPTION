Package: dragonking
Type: Package
Title: Statistical Tools to Identify Dragon Kings
Version: 0.1.0
Authors@R: c(person("Raoul", "Wadhwa", email = "raoulwadhwa@gmail.com", role = c("aut", "cre")),
             person("Christian", "Kelley", role = "aut"),
